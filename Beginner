        for(int i=0;i<size;i++)
        {
            a[i]=ma.nextInt();
        }
        int max1=0;
        int max2=0;
        for(int i=0;i<a.length;i++)
        {
            if(a[i]>max1)
            {
                max2=max1;
                max1=a[i];
