11.SORTING OF FACTORS IN DESCENDING ORDER
Sample input 
2
1 2

Sample output
2 1

import java.util.*;
public class Main
{
    public static void main(String args[])
    {
        Scanner ma=new Scanner(System.in);
        int a=ma.nextInt();
        int b[]=new int[a];
        for(int i=0;i<a;i++)
        {
            b[i]=ma.nextInt();
        }
        Arrays.sort(b);
        for(int i=a-1;i>=0;i--)
        {
            System.out.print(b[i]+" ");
        }
    }
}


12.CHARACTER MULTIPLIER
Sample input
a1b2z3i4u5

Sample output
abbzzziiiiuuuuu

import java.util.*;
public class Main
{
    public static void main(String args[])
    {
        Scanner ma=new Scanner(System.in);
        String s=ma.nextLine();
        String a="";
        for(int i=0;i<s.length();i=i+2)
        {
            int n=Character.getNumericValue(s.charAt(i+1));
            for(int j=0;j<n;j++)
            {
                a=a+s.charAt(i);
            }
        }
       System.out.print(a);
    }
}


