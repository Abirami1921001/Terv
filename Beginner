*

import java.util.*;
public class Main
{
    public static void main(String args[])
    {
        Scanner ma=new Scanner(System.in);
        int a=ma.nextInt();
        int b=a+(a-1);
        for(int i=1;i<=a;i++)
        {
            for(int j=0;j<b;j++)
            {
                System.out.print("*");
            }
            System.out.println();
            b=b-2;
        }
    }
}


