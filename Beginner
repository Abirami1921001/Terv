import java.util.*;
public class Main
{
    public static void main(String args[])
    {
        Scanner ma=new Scanner(System.in);
        int a=ma.nextInt();
        int count=0;
        for(int i=1;i<=a;i++)
        {
            if(a%i==0)
            {
            count++;
            }
        }
        if(count==2)
            System.out.println("Yes");
        else
            System.out.println("No");
    }
