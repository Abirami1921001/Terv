import java.util.*;
public class Main
{
    public static void main(String args[])
    {
        Scanner ma=new Scanner(System.in);
        int a=ma.nextInt();
        int alpha=65;
        for(int i=0;i<a;i++)
        {
            for(int j=0;j<=i;j++)
            {
                System.out.print((char)(alpha+j));
            }
            System.out.println();
        }
    }
}


17.RIGHT ANGLED INVERTED TRIANGLE
