7.DECIMAL TO BINARY
Sample input
10

Sample output
1010

import java.util.*;
public class Main
{
    public static void main(String args[])
    {
        Scanner ma=new Scanner(System.in);
        int a=ma.nextInt();
        int b[]=new int [a];
        int i=0;
        while(a>0)
        {
            b[i++]=a%2;
            a=a/2;
        }
        for(int j=i-1;j>=0;j--)
        {
            System.out.print(b[j]);
        }
    }
}


8.PALINDROME STRING OR NOT
Sample input
madam

Sample output
Yes

import java.util.*;
public class Main
{
    public static void main(String args[])
    {
        Scanner ma=new Scanner(System.in);
        String s1=ma.nextLine();
        String s2="";
        for(int i=s1.length()-1;i>=0;i--)
        {
            s2=s2+s1.charAt(i);
        }
        if(s1.equals(s2))
        {
            System.out.print("Yes");
        }
        else
        {
            System.out.print("No");
        }
    }
}


9.DECIMAL TO OCTAL
Sample input
12

Sample output
14

import java.util.*;
public class Main
{
    public static void main(String args[])
    {
        Scanner ma=new Scanner(System.in);
        int a=ma.nextInt();
        int rem,s=1,o=0;
        while(a!=0)
        {
            o=o+(a%8)*s;
            a=a/8;
            s=s*10;
        }
        System.out.print(o);
    }
}


