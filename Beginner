121

Sample output
Yes

import java.util.*;
public class Main
{
    public static void main(String args[])
    {
        Scanner ma=new Scanner(System.in);
        int a=ma.nextInt();
        int temp=a;
        int b=0;
        while(a>0)
        {
            b=b*10+a%10;
            a=a/10;
        }
        if(b!=temp)
        {
            System.out.println("No");
        }
        else
        System.out.println("Yes");
    }
}


